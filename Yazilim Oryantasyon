import java.util.Scanner;

public class Ã–dev7 {
    
    public static void main(String[] args) {
        //Beden kitle indeksinizi hesaplayan java kodu yazÄ±nÄ±z.
        Scanner scan = new Scanner(System.in);
        
        System.out.print("Boyunuzu giriniz (1,75) : ");
        float boy = scan.nextFloat();
        System.out.print("Kilonuzu giriniz: ");
        float kilo = scan.nextFloat();
        
        float bki = kilo / (boy*boy);
        System.out.println("Beden Kitle Ä°ndeksiniz = " + bki);
        
        if(bki < 18) {
            System.out.println("ZayÄ±f");
        }
        else if(bki < 25) {
            System.out.println("Normal");
        }
        else if(bki < 30) {
            System.out.println("Hafif ÅŸiÅŸman");
        }
        else if(bki < 35) {
            System.out.println("Sisman");
        }
        else {
            System.out.println("Obez");
        }
    }
}





import java.util.Scanner;

public class Ã–dev8 {
    
    public static void main(String[] args) {
        //Hesap makinesini java kodu ile yazÄ±nÄ±z.
        Scanner scan = new Scanner(System.in);
        
        
        System.out.print("1. SayÄ±yÄ± giriniz: ");
        int sayi1 = scan.nextInt();
        System.out.print("2. SayÄ±yÄ± giriniz: ");
        int sayi2 = scan.nextInt();
        
        System.out.println("****Ä°SLEMÄ°NÄ°ZÄ° SEÃ‡Ä°NÄ°Z***");
        System.out.println("1. TOPLAMA");
        System.out.println("2. Ã‡IKARMA");
        System.out.println("3. Ã‡ARPMA");
        System.out.println("4. BÃ–LME");
        
        System.out.print("Secimininizi giriniz: ");
        int secim = scan.nextInt();
        
        if(secim == 1) {
            System.out.println("Toplama isleminin sonucu = " + (sayi1 + sayi2));
        }
        else if(secim == 2) {
            System.out.println("Ã‡Ä±karma isleminin sonucu = " + (sayi1 - sayi2));
        }
        else if(secim == 3) {
            System.out.println("Carpma isleminin sonucu = " + (sayi1 * sayi2));
        }
        else if(secim == 4) {
            System.out.println("BÃ¶lme isleminin sonucu = " + (float)sayi1 / sayi2);
        }
        else {
            System.out.println("YanlÄ±s secim yaptÄ±nÄ±z. Seciminizi kontrol ediniz.");
        }
    }
}






public class Ã–dev9 {
    
    public static void main(String[] args) {
        //1' den 10' a kadar olan Ã§arpÄ±m tablosunu java kodu ile gÃ¶steriniz.
        for(int i = 1; i <= 10; i++)
        {
            for(int j = 1; j < 11; j++)
            {
                System.out.println(i + "x" + j + "= " + (i * j));
            }
            System.out.println("\n");
        }
    }
}




import java.util.Scanner;

public class Inspimo {
	public static void main(String []args){
		//ÃœÃ§genin bir kenarÄ±nÄ±n diÄŸer kenarlarÄ±yla olan iliÅŸkisini gÃ¶steren bir java kodu yazÄ±nÄ±z.
		Scanner sc= new Scanner (System.in);
		System.out.print("ÃœÃ§genin kenarÄ± a girin: ");
		int a = sc.nextInt();
		System.out.print("ÃœÃ§genin kenarÄ± b girin: ");
		int b = sc.nextInt();
		System.out.print("ÃœÃ§genin kenarÄ± c girin: ");
		int c = sc.nextInt();
		int sayi1 = b-c;
		int sayi2 = b+c;
		System.out.println(Math.abs(sayi1) + " < " + a + " < " + sayi2);
		if (sayi1 > sayi2 && sayi1 == a || sayi2 == a){
			System.out.println("ÃœÃ§gen oluÅŸturulamadÄ±.");
		}
		else {
			int sayi3 = a-c;
			int sayi4 = a+c;
			System.out.println(Math.abs(sayi3) + " < " + b + " < " + sayi4);
			if (sayi3 > sayi4 && sayi3 == b || sayi4 == b){
				System.out.println("ÃœÃ§gen oluÅŸturulamadÄ±.");
			}
			else {
				int sayi5 = a-b;
				int sayi6 = a+b;
				System.out.println(Math.abs(sayi5) + " < " + c + " < " + sayi6);
				if (sayi5 > sayi6 && sayi5 == c || sayi6 == c){
					System.out.println("ÃœÃ§gen oluÅŸturulamadÄ±.");
				}
			
		}
			
		
		}
		
	}
}
