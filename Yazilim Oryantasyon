import java.util.Scanner;


public class Ã–dev2 {
	public static void main(String[] args){
		
		// ğ‘ğ‘¥2 + ğ‘ğ‘¥ + ğ‘  = 0 denklemin kÃ¶klerini bulan programÄ± Java dilinde yazÄ±nÄ±z.
		Scanner sc = new Scanner(System.in);
		System.out.println("(ax2+bx+c) ikinci dereceden denklemin sabit sayÄ±larÄ±nÄ± giriniz.");
		System.out.print("a deÄŸerini gir: ");
		double a = sc.nextInt();
		System.out.print("b deÄŸerini gir: ");
		double b = sc.nextInt();
		System.out.print("c deÄŸerini gir: ");
		double c = sc.nextInt();
		
		double delta = (b*b)-(4*a*c);
		
		if (delta>0){
			double x1 = ((-1*b) - Math.sqrt(delta)) / (2*a);
			double x2 = ((-1*b) + Math.sqrt(delta)) / (2*a);
			System.out.println("x1= " + x1 + "x2= " + x2);
			
		}
		if (delta < 0){
			System.out.println("Denklemin gerÃ§el kÃ¶kÃ¼ yok.");
		}
		if (delta == 0){
			double x = (-1*b) / (2*a);
			System.out.println("Ã‡akÄ±ÅŸÄ±k kÃ¶kler var x1= x2= " + x);
		}
	}
	
}




import java.util.Random;
import java.util.Scanner;

public class Ã–dev3 {

	public static void main(String[] args) {
		//TaÅŸ-kaÄŸÄ±t-makas oyununu oynayabilecek bir program yazÄ±nÄ±z. 
		int kisi,pc;
		
		Random rnd=new Random();
		pc=rnd.nextInt(3)+1; 
		System.out.println("pc"+pc);
		System.out.println("1.taÅŸ");
		System.out.println("2.kaÄŸÄ±t");
		System.out.println("3.makas");
		System.out.println("LÃ¼tfen seÃ§iminizi yapÄ±nÄ±z");
		Scanner gelenioku=new Scanner(System.in);
		kisi=gelenioku.nextInt();
		if(kisi!=1 && kisi!=2 && kisi!=3){
			System.out.println("yanlÄ±ÅŸ bir seÃ§im yapÄ±lmÄ±ÅŸtÄ±r.");
		}
		else { 
			
			
			if(pc==kisi){
				System.out.println("Berabere!");
			}
			if(pc==1 && kisi==2){
				System.out.println("Sen kazandÄ±n!");
			}
			if(pc==1 && kisi==3){
				System.out.println("Yapay zeka kazandÄ±!");
			}
			if(pc==2 && kisi==1){
				System.out.println("Yapay zeka kazandÄ±!");
			}			
			if(pc==2 && kisi==3){
				System.out.println("Sen kazandÄ±n!");
			}
			if(pc==3 && kisi==1){
				System.out.println("Sen kazandÄ±n!");
			}
			if(pc==3 && kisi==2){
				System.out.println("Yapay zeka kazandÄ±!");
			}			
		}
	}
}




public class Ã–dev4 {
  
    public static void main(String[] args) { 
    	//ArdÄ±ÅŸÄ±k sayÄ±larÄ±n toplamÄ±nÄ± gÃ¶steren bir kod yazÄ±nÄ±z.
        int sayi = 400;
        int toplam = 0; 
